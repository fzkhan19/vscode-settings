  // Misc
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "editor.tabSize": 2,
  "editor.linkedEditing": true,
  "breadcrumbs.filePath": "last",
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "window.zoomLevel": 0.75,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "json.schemas": [],
  "terminal.integrated.enableImages": true,
  "terminal.integrated.gpuAcceleration": "on",
  "htmltagwrap.tag": "div",
  "gutterpreview.imagePreviewMaxHeight": 100,
  "gutterpreview.imagePreviewMaxWidth": 100,
  "files.associations": {
    ".env*": "dotenv",
    "*.css": "tailwindcss",
    "ostream": "cpp",
    "array": "cpp",
    "iostream": "cpp",
    "wrangler.json": "jsonc"
  },
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "dotenv.enableAutocloaking": false,
  "explorer.confirmPasteNative": false,
  "cody.autocomplete.enabled": false,
  "bootstrapAutocomplete.version": "5.3.2",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "C_Cpp.default.compilerPath": "C:\\msys64\\ucrt64\\bin\\g++.exe",
  "code-runner.runInTerminal": true,
  "extensions.ignoreRecommendations": true,
  "[tailwindcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  "explorer.incrementalNaming": "disabled",
  "window.density.editorTabHeight": "compact",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "cody.chat.defaultLocation": "sidebar",
  "cody.telemetry.level": "off",
  "supermaven.allowGitignore": true,
  "supermaven.enableDeleteSuggestions": false,
  "supermaven.enableFixWithSupermaven": false,
  "supermaven.enableJumpSuggestions": false,
  "[cpp]": {
