{
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "files.autoSaveDelay": 800,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.accessibilitySupport": "off",
  "diffEditor.ignoreTrimWhitespace": false,
  "update.mode": "none",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "html"
  },

  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editorAssociations": {
    "*.xlsx": "xlsxViewer.xlsx"
  },

  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,

  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.prettier": "explicit",
    "source.removeUnusedImports": "explicit",
    "source.addMissingImports.ts": "explicit",
    "source.organizeImports": "always",
    "source.sortImports": "always",
    "source.action.useSortedAttributes.biome": "explicit",
    "source.action.useSortedProperties.biome": "explicit",
    "source.action.useSortedKeys.biome": "explicit",
  },

  "eslint.format.enable": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.env.windows": {},
  "notebook.formatOnSave.enabled": true,
  "editor.fontLigatures": false,
  "terminal.integrated.defaultProfile.windows": "PowerShell",

  "cSpell.userWords": [
    "APPWRITE",
    "autofetch",
    "Autosize",
    "biomejs",
    "ciphertext",
    "classname",
    "Consolas",
    "Dockbar",
    "dotenv",
    "Faiz",
    "filepond",
    "filterdata",
    "firestore",
    "fullname",
    "Geocode",
    "Hardscape",
    "Hardscaping",
    "hdel",
    "Heathrow",
    "hincrby",
    "hookform",
    "isbot",
    "latlng",
    "lucide",
    "moxer",
    "nextjs",
    "oems",
    "onremovefile",
    "opengraph",
    "paramkey",
    "pnpx",
    "quickfix",
    "sadd",
    "Shadcn",
    "SIDENAV",
    "smembers",
    "sonner",
    "srem",
    "staticity",
    "tailwindcss",
    "tailwindstyles",
    "tanstack",
    "Tawk",
    "toastify",
    "todos",
    "trpc",
    "ttfb",
    "ttlb",
    "typecheck",
    "unoptimized",
    "upserted",
    "waypts",
    "webp",
    "Wordcloud",
    "Xvalues",
    "Yvalues"
  ],

  "color-highlight.matchRgbWithNoFunction": true,
  "color-highlight.matchWords": true,
  "color-highlight.rgbWithNoFunctionLanguages": ["*"],
  "color-highlight.markRuler": true,
  "color-highlight.markerType": "dot-before",

  "workbench.editor.labelFormat": "short",
  "explorer.sortOrder": "filesFirst",
  "workbench.tree.indent": 12,
  "editor.inlineSuggest.suppressSuggestions": true,
  "cody.commandCodeLenses": true,
  "explorer.confirmDelete": false,
  "cody.commandHints.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "vim.replaceWithRegister": true,
  "workbench.settings.applyToAllProfiles": [],

  // open json editor for settings
  "workbench.settings.editor": "json",

  // Theme
  "workbench.colorTheme": "Aura Dark (Soft Text)",
  "workbench.iconTheme": "moxer-icons",

  // Change font
  "editor.fontFamily": "'GeistMono Nerd Font', Consolas",
  "scm.inputFontFamily": "'GeistMono Nerd Font', Consolas",
  "chat.editor.fontFamily": "'GeistMono Nerd Font', Consolas",
  "debug.console.fontFamily": "'GeistMono Nerd Font', Consolas",
  "editor.codeLensFontFamily": "'GeistMono Nerd Font', Consolas",
  "notebook.output.fontFamily": "'GeistMono Nerd Font', Consolas",
  "markdown.preview.fontFamily": "'GeistMono Nerd Font', Consolas",
  "editor.inlayHints.fontFamily": "'GeistMono Nerd Font', Consolas",
  "editor.inlineSuggest.fontFamily": "'GeistMono Nerd Font'', Consolas",

  // Font size
  "editor.fontSize": 13,
  "editor.lineHeight": 1.43,
  "editor.letterSpacing": 0.1,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.cursorBlinking": "smooth",
  "terminal.integrated.fontSize": 12,
  "editor.scrollbar.vertical": "auto",
  "window.titleBarStyle": "custom",
  "editor.scrollbar.horizontal": "auto",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "workbench.tips.enabled": false,

  // APC
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": false,
    "size": 28,
    "itemMargin": 2
  },
  "apc.electron": {
    "titleBarStyle": "hidden"
  },
  "apc.header": {
    "height": 35,
    "fontSize": 12
  },
  "apc.listRow": {
    "lists": [
      "explorer-folders-view",
      "quick-input-list",
      "customview-tree",
      "results",
      "open-editors",
      "outline-tree",
      "scm-view",
      "debug-view-content",
      "debug-breakpoints",
      "tree"
    ],
    "height": 21,
    "fontSize": 12
  },

  "terminal.integrated.fontFamily": "JetBrainsMonoNL Nerd Font, Consolas",
  "apc.font.family": "'GeistMono Nerd Font'",
  "apc.monospace.font.family": "Consolas",
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 28,
    "fontSize": 12
  },

  "window.title": "${rootName}",

  "apc.imports": [
    "/C:/Users/Khan/Desktop/VsCode/style.css",
    "/C:/Users/Khan/Desktop/VsCode/script.css"
  ],

  // Aura theme customization
  "workbench.colorCustomizations": {
    "statusBar.noFolderBackground": "#110f17",
    "statusBar.border": "#110f17",
    "[Aura Dark]": {
      "editor.background": "#110f17",
      "editorCursor.foreground": "#c9b8e4",
      "terminal.background": "#110f17",
      "activityBar.background": "#110f17",
      "statusBar.background": "#110f17",
      "editorGroupHeader.tabsBackground": "#110f17",
      "tab.inactiveBackground": "#110f17"
    },
    "[Aura Dark (Soft Text)]": {
      "editor.background": "#110f17",
      "editorCursor.foreground": "#c9b8e4",
      "terminal.background": "#110f17",
      "activityBar.background": "#110f17",
      "statusBar.background": "#110f17",
      "editorGroupHeader.tabsBackground": "#110f17",
      "tab.inactiveBackground": "#181825"
    }
  },

  // Toggle excluded files
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/node_modules": false,
    "**/.next": false,
    "**/.husky": false,
    "**/drizzle": false,
    "**/dist": false,
    "**/.todo.md": false,
